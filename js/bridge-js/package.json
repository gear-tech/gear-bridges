{
  "name": "gear-bridge-js",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "Dmitri Osipov <dmith.osipov@gmail.com>",
  "homepage": "https://github.com/gear-tech/gear-bridges/tree/main/js/bridge-js#readme",
  "license": "GPL-3.0",
  "type": "module",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gear-tech/gear-bridges.git"
  },
  "bugs": {
    "url": "https://github.com/gear-tech/gear-bridges/issues"
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.cjs"
    }
  },
  "scripts": {
    "clean": "npx rimraf lib",
    "test": "npx vitest",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "swc ./src -d lib/esm --strip-leading-paths",
    "build:cjs": "swc ./src -d lib/cjs -C module.type=commonjs --out-file-extension cjs --strip-leading-paths",
    "build:types": "tsc -p tsconfig.types.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@chainsafe/ssz": "^1.2.1",
    "@ethereumjs/rlp": "^10.0.0",
    "@ethereumjs/trie": "^6.2.1",
    "@ethereumjs/util": "^10.0.0",
    "@gear-js/api": "^0.43.0",
    "@lodestar/types": "^1.33.0",
    "@polkadot/api": "^16.4.3",
    "sails-js": "^0.4.3",
    "sails-js-parser": "^0.4.3",
    "viem": "^2.33.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "sails-js-cli": "^0.4.3",
    "vitest": "^3.2.4"
  }
}
